{
  "expo": {
    "name": "WithLove",
    "slug": "WithLove",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/withLove.png",
    "scheme": "withlove",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#DFF6E3",
      "imageWidth": 200
    },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/withLove.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/withLove.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "WithLove accesses your photos to let you share your moments"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
