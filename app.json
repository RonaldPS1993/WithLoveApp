{
  "expo": {
    "name": "WithLove",
    "slug": "WithLove",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/WithLove.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#DFF6E3"
    },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/WithLove.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/WithLove.png"
    },
    "plugins": [],
    "experiments": {
      "typedRoutes": true
    }
  }
}
